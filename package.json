{
  "name": "vite-plugin-ftp-nova",
  "version": "1.0.0",
  "description": "a simple useful vite ftp plugin",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup"
  },
  "type": "module",
  "files": [
    "src"
  ],
  "homepage": "https://github.com/itfrogs/vite-plugin-ftp-nova",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itfrogs/vite-plugin-ftp-nova"
  },
  "keywords": [
    "vite",
    "vue3",
    "vue-next",
    "ftp",
    "deploy"
  ],
  "author": "ITFrogs (https://github.com/itfrogs/)",
  "license": "ISC",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "sourcemap": false,
    "dts": true,
    "splitting": false,
    "clean": true,
    "format": [
      "esm",
      "cjs"
    ]
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "ftp-deploy": "^2.4.7",
    "ora": "^8.1.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  },
  "devDependencies": {
    "vue": "^3.5.12",
    "@types/node": "^22.8.0",
    "tsup": "^8.3.4"
  }
}